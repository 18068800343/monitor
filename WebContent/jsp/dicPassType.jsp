<script type="text/javascript">
    $(document).ready(function () {
        // DO NOT REMOVE : GLOBAL FUNCTIONS!
        pageSetUp();
        initTable();
    });

    function initTable() {
        $.ajax({
            type: 'POST',
            url: '../DicStructTypeServlet',
            dataType: 'json',
            data: {
                type: "initPassType"
            },
            error: function (msg) {
                errMessage("请求DicStructTypeServlet失败");
            },
            success: function (json) {
                if (json.success == "fail") {
                    switch (json.error) {
                        case 1:
                            errMessage("没有数据");
                            break;
                        case 2:
                            errMessage("服务器错误");
                            break;
                        default:
                            break;
                    }
                    $('#dt_basic').find('tbody').empty();
                } else {
                    $('#dt_basic').find('tbody').empty();
                    var data = json.obj;
                    //alert(JSON.stringify(data));
                    for (var i = 0; i < data.length; i++) {
                        //table.row.add(data[i]).draw(true);
                        //alert(JSON.stringify(data2));
                        $('#dt_basic').append('<tr><td>' + data[i].pass_type_id + '</td><td>' + data[i].pass_type_name + '</td></tr>')
                    }
                }
            }
        });
    }


    function errMessage(info) {
        $.smallBox({
            title: "处理信息",
            content: "<i class='fa fa-clock-o'></i> <i>" + info + "</i>",
            color: "#C46A69",
            iconSmall: "fa fa-times fa-2x fadeInRight animated",
            timeout: 3000
        });
    }

    function successMessage(info) {
        $.smallBox({
            title: "处理信息",
            content: "<i class='fa fa-clock-o'></i> <i>" + info + "</i>",
            color: "#659265",
            iconSmall: "fa fa-times fa-2x fadeInRight animated",
            timeout: 3000
        });
    }

